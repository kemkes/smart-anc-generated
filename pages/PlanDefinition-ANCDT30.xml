    <title value="ANC.DT.30 General risk reduction counselling"/>
    <trigger>
      <type value="named-event"/>
      <name value="ANC.B9. Conduct laboratory tests and imaging"/>
    </trigger>
    <action id="1">
      <title value="Provide counselling on Rh factor negative"/>
      <description value="Provide counselling on Rh factor negative"/>
      <textEquivalent value="– Woman is at risk of alloimmunization if the baby's father is Rh positive or unknown&#xA;– Proceed with local protocol to investigate sensitization and the need for referral &#xA;– If non-sensitized, woman should receive anti-D prophylaxis postnatally if the baby is Rh positive"/>
      <documentation>
        <type value="citation"/>
        <label value="WHO ANC recommendations (2016): C.3. Antenatal anti-D immunoglobulin administration (2)"/>
      </documentation>
      <condition>
        <kind value="applicability"/>
        <expression>
          <description value="Rh factor&quot; = &quot;Rh negative&quot;"/>
          <language value="text/cql-identifier"/>
          <expression value="Should Provide counselling on Rh factor negative"/>
        </expression>
      </condition>
    </action>
    <action id="2">
      <title value="Provide counselling on intrapartum antibiotic to prevent early neonatal Group B Streptococcus (GBS) infection"/>
      <description value="Provide counselling on intrapartum antibiotic to prevent early neonatal Group B Streptococcus (GBS) infection"/>
      <textEquivalent value="Pregnant women with GBS colonization should receive intrapartum antibiotic administration to prevent early neonatal GBS infection"/>
      <documentation>
        <type value="citation"/>
        <label value="WHO ANC recommendations (2016): C.1: Antibiotics for asymptomatic bacteriuria (ASB) (2)"/>
      </documentation>
      <condition>
        <kind value="applicability"/>
        <expression>
          <description value="Midstream urine culture (recommended)&quot; = &quot;Positive – Group B Streptococcus (GBS)&quot;"/>
          <language value="text/cql-identifier"/>
          <expression value="Should Provide counselling on intrapartum antibiotic to prevent early neonatal Group B Streptococcus (GBS) infection"/>
        </expression>
      </condition>
    </action>
