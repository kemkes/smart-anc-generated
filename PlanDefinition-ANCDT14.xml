    <title value="ANC.DT.14 Other"/>
    <trigger>
      <type value="named-event"/>
      <name value="ANC.B10.4. Diagnosis and treatment"/>
    </trigger>
    <action id="1">
      <title value="Follow-up steps/investigations: clinician's discretion"/>
      <description value="Follow-up steps/investigations: clinician's discretion"/>
      <textEquivalent value="Hematocrit levels too low!"/>
      <documentation>
        <type value="citation"/>
        <label value="Managing complications guide (IMPAC) (2017): Sections S-7, S-47, S-151 (5)"/>
      </documentation>
      <condition>
        <kind value="applicability"/>
        <expression>
          <description value="&quot;Hematocrit (Ht)&quot; &lt; 20%"/>
          <language value="text/cql-identifier"/>
          <expression value="Should Follow-up steps/investigations: clinician's discretion"/>
        </expression>
      </condition>
    </action>
    <action id="2">
      <title value="Follow-up steps/investigations: clinician's discretion"/>
      <description value="Follow-up steps/investigations: clinician's discretion 2"/>
      <textEquivalent value="White blood cell count too high!"/>
      <documentation>
        <type value="citation"/>
        <label value="Managing complications guide (IMPAC) (2017): Section S-151 (5)"/>
      </documentation>
      <condition>
        <kind value="applicability"/>
        <expression>
          <description value="&quot;White blood cell (WBC) count&quot; &gt; 16 000"/>
          <language value="text/cql-identifier"/>
          <expression value="Should Follow-up steps/investigations: clinician's discretion 2"/>
        </expression>
      </condition>
    </action>
    <action id="3">
      <title value="Follow-up steps/investigations: clinician's discretion"/>
      <description value="Follow-up steps/investigations: clinician's discretion 3"/>
      <textEquivalent value="Platelet count too low! "/>
      <documentation>
        <type value="citation"/>
        <label value="Managing complications guide (IMPAC) (2017): Section S-53 (5)"/>
      </documentation>
      <condition>
        <kind value="applicability"/>
        <expression>
          <description value="&quot;Platelet count&quot; &lt; 100 000 cells/microlitre (μl)"/>
          <language value="text/cql-identifier"/>
          <expression value="Should Follow-up steps/investigations: clinician's discretion 3"/>
        </expression>
      </condition>
    </action>
  </action>
