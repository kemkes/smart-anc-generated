         fhir:RelatedArtifact.label [ fhir:value "Pregnancy, childbirth, postpartum and newborn care guide (IMPAC) (2015): recommendation C3 (1)\nManaging complications guide (IMPAC) (2017): Section S-53, Table S-12 (5)" ]
       ];
       fhir:PlanDefinition.action.condition [
         fhir:index 0;
         fhir:PlanDefinition.action.condition.kind [ fhir:value "applicability" ];
